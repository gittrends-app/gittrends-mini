{
  "name": "@gittrends/cli",
  "version": "1.0.0",
  "main": "dist",
  "types": "dist",
  "license": "MIT",
  "scripts": {
    "cli": "node -r @gittrends/env dist/cli.js",
    "build": "yarn install && rm -rf dist tsconfig.tsbuildinfo && tsc",
    "eslint": "eslint --color",
    "add-repositories": "node -r @gittrends/env dist/scripts/add-repositories.js",
    "schedule": "node -r @gittrends/env dist/scripts/schedule.js",
    "update": "node -r @gittrends/env dist/scripts/update.js",
    "migrations": "node -r @gittrends/env dist/scripts/migrations.js",
    "bull-board": "node -r @gittrends/env dist/scripts/bull-queue-board.js",
    "clinic:update": "clinic bubbleprof -- node dist/scripts/update.js",
    "dev": "node -r ts-node/register -r tsconfig-paths/register -r @gittrends/env src/cli.ts",
    "dev:add-repositories": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/add-repositories.ts",
    "dev:schedule": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/schedule.ts",
    "dev:update": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/update.ts",
    "dev:migrations": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/migrations.ts",
    "dev:bull-board": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/bull-queue-board.ts",
    "migrate:make": "knex migrate:make --migrations-directory src/config/migrations"
  },
  "dependencies": {
    "@bull-board/express": "^4.3.2",
    "async": "^3.2.4",
    "better-sqlite3": "^7.6.2",
    "bluebird": "^3.7.2",
    "bullmq": "^2.1.3",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.0",
    "consola": "^2.15.3",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "prettyjson": "^1.2.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@gittrends-app/github-proxy-server": "gittrends-app/github-proxy-server",
    "@types/async": "^3.2.15",
    "@types/cli-progress": "^3.11.0",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/prettyjson": "^0.0.30",
    "clinic": "^12.0.0",
    "sync-dotenv": "^2.7.0"
  }
}
