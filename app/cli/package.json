{
  "name": "@gittrends/cli",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "eslint": "eslint --color",
    "add-repositories": "node -r dotenv/config dist/scripts/add-repositories.js",
    "schedule": "node -r dotenv/config dist/scripts/schedule.js",
    "update": "node -r dotenv/config dist/scripts/update.js",
    "migrations": "node -r dotenv/config dist/scripts/migrations.js",
    "clinic:update": "clinic bubbleprof -- node dist/scripts/update.js",
    "dev:add-repositories": "ts-node -r dotenv/config -T src/scripts/add-repositories.ts",
    "dev:schedule": "ts-node -r dotenv/config -T src/scripts/schedule.ts",
    "dev:update": "ts-node -r dotenv/config -T src/scripts/update.ts",
    "dev:migrations": "ts-node -r dotenv/config -T src/scripts/migrations.ts",
    "migrate:make": "knex migrate:make --migrations-directory src/config/migrations -x ts"
  },
  "dependencies": {
    "@gittrends/lib": "link:../../shared/lib",
    "async": "^3.2.4",
    "better-sqlite3": "^7.6.2",
    "bluebird": "^3.7.2",
    "bull": "^4.9.0",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.2",
    "glob": "^8.0.3",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "prettyjson": "^1.2.5",
    "sqlite3": "^5.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@gittrends-app/github-proxy-server": "gittrends-app/github-proxy-server",
    "@types/async": "^3.2.15",
    "@types/bull": "^3.15.9",
    "@types/cli-progress": "^3.11.0",
    "@types/glob": "^8.0.0",
    "@types/prettyjson": "^0.0.30",
    "clinic": "^12.0.0",
    "sync-dotenv": "^2.7.0"
  }
}
