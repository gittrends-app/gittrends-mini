{
  "name": "@gittrends/cli",
  "version": "1.0.0",
  "main": "dist",
  "types": "dist",
  "license": "MIT",
  "scripts": {
    "build": "yarn install && rm -rf dist tsconfig.tsbuildinfo && tsc && ln -s ./src/scripts/queue-board/index.html ./dist/scripts/queue-board/index.html && webpack --mode production",
    "cli": "node --expose-gc -r @gittrends/env dist/cli.js",
    "cli-runtime": "pm2-runtime dist/cli.js --node-args=\"--expose-gc -r @gittrends/env\" --stop-exit-codes=0 --",
    "dev": "node --expose-gc -r ts-node/register -r tsconfig-paths/register -r @gittrends/env src/cli.ts",
    "dev:reproduce": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/reproduce.ts",
    "dev:migrate-make": "knex migrate:make --migrations-directory src/config/migrations",
    "dev:webpack": "yarn cli bull-board & webpack --mode production",
    "dev:webpack-serve": "yarn dev bull-board & webpack serve --open"
  },
  "dependencies": {
    "@bull-board/express": "^4.3.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^5.17.8",
    "@types/webpack-dev-server": "^4.7.2",
    "apicache": "^1.6.3",
    "async": "^3.2.4",
    "better-sqlite3": "^9.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "bullmq": "^2.1.3",
    "chalk": "^4.1.2",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.0",
    "consola": "^3.2.3",
    "express": "^4.19.1",
    "glob": "^8.0.3",
    "inquirer": "^8.2.3",
    "internal-ip": "^6.2.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "numeral": "^2.0.6",
    "p-queue": "^6",
    "pg": "^8.8.0",
    "pm2": "^5.2.0",
    "prettyjson": "^1.2.5",
    "swr": "^1.3.0",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.1",
    "@commitlint/cli": "^19.2.1",
    "@gittrends-app/github-proxy-server": "gittrends-app/github-proxy-server",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@types/apicache": "^1.6.1",
    "@types/async": "^3.2.15",
    "@types/bluebird": "^3.5.38",
    "@types/cli-progress": "^3.11.0",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.2",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/node": "20.11.30",
    "@types/numeral": "^2.0.2",
    "@types/prettyjson": "^0.0.33",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.0.7",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-loader": "^9.1.3",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "sync-dotenv": "^2.7.0",
    "ts-loader": "^9.4.1",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
