{
  "name": "@gittrends/cli",
  "version": "1.0.0",
  "main": "dist",
  "types": "dist",
  "license": "MIT",
  "scripts": {
    "cli": "node --expose-gc -r @gittrends/env dist/cli.js",
    "cli-runtime": "pm2-runtime dist/cli.js --node-args=\"--expose-gc -r @gittrends/env\" --stop-exit-codes=0 --",
    "build": "yarn install && rm -rf dist tsconfig.tsbuildinfo && tsc && ln -s ./src/scripts/queue-board/index.html ./dist/scripts/queue-board/index.html ",
    "eslint": "eslint --color",
    "add-repositories": "node -r @gittrends/env dist/scripts/add-repositories.js",
    "schedule": "node -r @gittrends/env dist/scripts/schedule.js",
    "update": "node -r @gittrends/env dist/scripts/update.js",
    "migrations": "node -r @gittrends/env dist/scripts/migrations.js",
    "bull-board": "node -r @gittrends/env dist/scripts/queue-board/index.js",
    "clinic:update": "clinic bubbleprof -- node dist/scripts/update.js",
    "dev": "node --expose-gc -r ts-node/register -r tsconfig-paths/register -r @gittrends/env src/cli.ts",
    "dev:add-repositories": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/add-repositories.ts",
    "dev:schedule": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/schedule.ts",
    "dev:update": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/update.ts",
    "dev:migrations": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/migrations.ts",
    "dev:reproduce": "ts-node -r tsconfig-paths/register -r @gittrends/env src/scripts/reproduce.ts",
    "dev:bull-board": "ts-node-dev -r tsconfig-paths/register -r @gittrends/env -T src/scripts/queue-board/index.ts",
    "migrate:make": "knex migrate:make --migrations-directory src/config/migrations",
    "webpack": "webpack --mode production",
    "webpack:serve": "webpack serve --open"
  },
  "dependencies": {
    "@bull-board/express": "^4.3.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@mui/x-data-grid": "^5.17.8",
    "@types/webpack-dev-server": "^4.7.2",
    "apicache": "^1.6.3",
    "async": "^3.2.4",
    "better-sqlite3": "^7.6.2",
    "bluebird": "^3.7.2",
    "bullmq": "^2.1.3",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.0",
    "consola": "^2.15.3",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "inquirer": "^8.2.3",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pm2": "^5.2.0",
    "prettyjson": "^1.2.5",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@gittrends-app/github-proxy-server": "gittrends-app/github-proxy-server",
    "@types/apicache": "^1.6.1",
    "@types/async": "^3.2.15",
    "@types/cli-progress": "^3.11.0",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.5",
    "@types/prettyjson": "^0.0.30",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "clinic": "^12.0.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "sync-dotenv": "^2.7.0",
    "ts-loader": "^9.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
